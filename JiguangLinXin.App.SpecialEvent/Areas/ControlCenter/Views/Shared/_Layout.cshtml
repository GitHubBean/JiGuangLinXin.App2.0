    <!-- page specific plugin scripts -->
    @RenderSection("footerScript", false)

    <!--[if lte IE 8]>
      <script src="/assets/js/excanvas.min.js"></script>
    <![endif]-->
    <!-- ace scripts -->
    <script src="/assets/js/ace-elements.min.js"></script>
    <script src="/assets/js/ace.min.js"></script>

    <!-- inline scripts related to this page -->
    <script>
        $(function () {
            $('[data-rel=tooltip]').tooltip();
            $('[data-rel=popover]').popover({ html: true });

        });
        $(window).load(function () {
            //var hr = window.location.pathname;
            var hr = '@ViewBag.ParentUrl';
            
            //var url = Cookies.get("menuUrl");
            hr = hr || '/CP/console';
            $('a[name="am"]').each(function (i, o) {
                var curObj = $(o);
                var href = curObj.attr('href');
                //console.log(hr+'**'+href);
                if (href == hr) {
                    curObj.parent('li').addClass('active');
                    var sm = curObj.parents('.submenu');
                    if (sm) {
                        sm.show();
                        sm.parent().addClass('active open');
                    }
                }
            });
        });
    </script>
    @RenderSection("footer", false)
</body>
</html>
