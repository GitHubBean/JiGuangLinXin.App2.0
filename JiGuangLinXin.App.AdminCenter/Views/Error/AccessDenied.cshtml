@{
    ViewBag.Title = "拒绝访问";
}
<div class="main-content">
    <div class="breadcrumbs" id="breadcrumbs">
        <ul class="breadcrumb">
            <li>
                <i class="ace-icon fa fa-home home-icon"></i>
                <a href="@Url.Action("Index","Console")">控制台</a>
            </li>

            <li>
                <a href="###">拒绝访问</a>
            </li>
            <li class="active">AccessDenied</li>
        </ul><!-- .breadcrumb -->
    </div>

    <div class="page-content">
        <div class="row">
            <div class="col-xs-12">
                <!-- PAGE CONTENT BEGINS -->

                <div class="error-container">
                    <div class="well">
                        <h1 class="grey lighter smaller">
                            <span class="blue bigger-125">
                                <i class="ace-icon fa fa-random"></i>
                                AccessDenied
                            </span>
                            拒绝访问！
                        </h1>

                        <hr>
                        <h3 class="lighter smaller">
                            你暂无权进行此操作！
                            <i class="ace-icon fa fa-wrench icon-animated-bell bigger-125"></i>
                        </h3>

                        <div class="space"></div>

                        <div>
                            <h3 class="red lighter smaller">若您需要进行此项操作，请联系技术人员为您配置！</h3>


                            <ul class="list-unstyled spaced inline bigger-110 margin-15">
                                <li>

                                    <div class="alert alert-warning">
                                        Controller:@TempData["Controller"] / Action:@TempData["Action"]
                                    </div>
                                </li>
                                <li>
                                    <i class="ace-icon fa fa-hand-o-right blue"></i>
                                    联系方式(QQ)：8888888888
                                </li>
                            </ul>
                        </div>

                        <hr>
                        <div class="space"></div>

                        <div class="center">
                            <a href="javascript:history.go(-1);" class="btn btn-grey">
                                <i class="ace-icon fa fa-arrow-left"></i>
                                返回上一步
                            </a>

                            <a href="@Url.Action("Index", "Console")" class="btn btn-primary">
                                <i class="ace-icon fa fa-dashboard"></i>
                                回首页
                            </a>
                        </div>
                    </div>
                </div>

                <!-- PAGE CONTENT ENDS -->
            </div><!-- /.col -->
        </div><!-- /.row -->
    </div><!-- /.page-content -->
</div>