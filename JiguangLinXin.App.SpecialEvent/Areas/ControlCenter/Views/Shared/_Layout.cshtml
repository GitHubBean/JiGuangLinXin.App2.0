                    <!-- #section:basics/navbar.user_menu -->
                    <li class="light-blue">
                        <a data-toggle="dropdown" href="#" class="dropdown-toggle">
                            <img class="nav-user-photo" src="@string.Format("/static/avatar/{0}.png",ViewBag.UId.ToString("N"))" alt="Jason's Photo" onerror="this.src = '/static/avatar/3cee192165ae49829101061405c7de4c.png'" />
                            <span class="user-info">
                                <small>欢迎,</small>
                                @ViewBag.UName
